{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "alwaysStrict": true,
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ]
    }
  },
  "exclude": [
    "dist/"
  ]
}